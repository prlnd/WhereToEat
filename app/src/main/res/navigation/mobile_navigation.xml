<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/menu_list">

    <fragment
        android:id="@+id/menu_list"
        android:name="com.example.wheretoeat.ui.prev.list.ListFragment"
        android:label="List"
        tools:layout="@layout/fragment_list" />

    <fragment
        android:id="@+id/menu_add"
        android:name="com.example.wheretoeat.ui.prev.favorite.FavoriteFragment"
        android:label="Add"
        tools:layout="@layout/fragment_user" />

    <fragment
        android:id="@+id/menu_user"
        android:name="com.example.wheretoeat.ui.prev.user.UserFragment"
        android:label="Family"
        tools:layout="@layout/fragment_favorite" />
</navigation>